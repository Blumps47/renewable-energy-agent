MAX_CONTEXT_LENGTH=8000

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
API_KEY=your_api_key_for_authentication

# External Services
WEATHER_API_KEY=your_weather_api_key
NREL_API_KEY=your_nrel_api_key 